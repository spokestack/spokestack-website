{
  "presets": [[
    "babel-preset-gatsby",
    {
      "targets": {
        "browsers": [">0.25%", "not dead", "IE 11"]
      }
    }
  ]],
  "plugins": [
    "graphql-tag",
    "inline-import",
    "transform-inline-environment-variables",
    ["prismjs", {
      "languages": ["javascript", "swift", "java", "kotlin", "python"],
      "plugins": ["line-numbers", "show-language"],
      "css": false
    }]
  ],
  "env": {
    "production": {
      "plugins": ["transform-remove-console"]
    }
  }
}
